{
  "extends": "solhint:recommended",
  "plugins": [],
  "rules": {
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "compiler-version": ["error", "=0.8.9"],
    "func-name-mixedcase": "off",
    "no-inline-assembly": "off",
